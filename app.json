{
  "name": "shadowsocks-heroku",
  "description": "Deploy shadowsocks server to Heroku.",
  "stack": "container",
  "env": {
    "SS_PASSWORD": {
      "value": "freedom_is_not_free",
      "description": "shadowsocks password.",
      "required": true
    },
    "SS_ENCRYPT": {
      "value": "chacha20-ietf-poly1305",
      "description": "shadowsocks encrypt method.",
      "required": false
    },
    "SS_PLUGIN": {
      "value": "xray-plugin",
      "description": "shadowsocks sip003 plugin.",
      "required": false
    },
    "SS_PLUGIN_OPTS": {
      "value": "server;fast-open",
      "description": "shadowsocks sip003 plugin options.",
      "required": false
    }
  }
}
